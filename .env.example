# Environment Variables
# Copy this file to .env and fill in your actual values

# Google Service Account JSON key (base64 encoded for GitHub Actions)
GOOGLE_SERVICE_ACCOUNT_KEY=

# For local development, you can also use the path to your service account JSON file
GOOGLE_SERVICE_ACCOUNT_PATH=./config/service-account.json

# Database Sheet ID - The Google Sheet containing the Commander Database
# Get this from your Commander Database sheet URL:
# https://docs.google.com/spreadsheets/d/{DATABASE_SHEET_ID}/edit
DATABASE_SHEET_ID=

# Logging configuration
LOG_LEVEL=info
